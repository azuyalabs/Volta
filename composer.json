{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "azuyalabs/yasumi": "^2.1",
    "cache/predis-adapter": "^1.0",
    "cknow/laravel-money": "^3.4",
    "cviebrock/eloquent-sluggable": "^4.8",
    "erusev/parsedown-extra": "^0.7",
    "fideloper/proxy": "~4.1",
    "gmopx/laravel-owm": "^0.1",
    "knplabs/github-api": "^2.11",
    "laravel/framework": "5.8.*",
    "laravel/passport": "^7.0",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.7",
    "nothingworks/blade-svg": "^0.3",
    "php-http/guzzle6-adapter": "^1.1",
    "predis/predis": "^1.1",
    "punic/punic": "^3.3",
    "pusher/pusher-php-server": "3.3",
    "spatie/laravel-model-status": "^1.6",
    "spatie/laravel-permission": "^2.30",
    "symfony/dom-crawler": "^4.2",
    "league/plates": "^3.3"
  },
  "require-dev": {
    "doctrine/dbal": "^2.9",
    "filp/whoops": "~2.3",
    "friendsofphp/php-cs-fixer": "^2.14",
    "fzaninotto/faker": "~1.8",
    "mockery/mockery": "~1.2",
    "nunomaduro/collision": "~3.0",
    "nunomaduro/larastan": "^0.3",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "bootstrap/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "test": "vendor/bin/phpunit",
    "format": [
      "vendor/bin/php-cs-fixer fix"
    ],
    "analyse": [
      "@php artisan code:analyse --level=5"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
